<Ui xmlns="http://www.blizzard.com/wow/ui/"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\..\FrameXML\UI.xsd">
	<Script File="QrcodeAPI.lua"/>
	<Frame name="QR">
		<Frames>
			<Frame name="viewFrame" parentKey="viewFrame" enableKeyboard="true" frameStrata="DIALOG" hidden="true">
				<Layers>
					<Layer>
					<Texture setAllPoints="true">
						<Color r="1.0" g="1.0" b="1.0" a="1.0"/>
					</Texture>
					</Layer>	
				</Layers>	
				<Anchors>
					<Anchor point="CENTER" relativeTo="UIParent"/>
				</Anchors>
				<Size>
					<AbsDimension x="128" y="128"/>
				</Size>
				<Frames>
					<Frame name="dotHolder" parentKey="dotHolder">
						<Size>
							<AbsDimension x="160" y="160"/>
						</Size>
						<Anchors>
							<Anchor point="CENTER"/>	
						</Anchors>
					</Frame>	
				</Frames>
				<Scripts>
					<OnKeyDown>
						self:Hide();
					</OnKeyDown>
				</Scripts>			
			</Frame>	
		</Frames>	
		<Scripts>
  		<OnLoad>init();</OnLoad>
  	</Scripts>
  	<Size x="1" y="1"/>
  	<Anchors>
			<Anchor point="CENTER" relativeTo="UIParent"/>
		</Anchors>
  </Frame>
</Ui> 